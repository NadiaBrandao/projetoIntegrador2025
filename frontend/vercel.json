{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "builds": [
      {
        "src": "angular.json",
        "use": "@vercel/angular"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html",
        "status": 200
      }
    ]
  }

  